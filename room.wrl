#VRML V2.0 utf8


Background {
    groundColor 0 1 0 
    skyColor 0.1 0.8 0.9
				  
}


#Wall
Transform {
    children [
        #floor
        Transform {
            translation 7.5 0 5.5
            children [
                Shape {
                    geometry Box {
                        size 15 0.1 11
                    }
					appearance Appearance {
						# material Material {
						# 	ambientIntensity .103
						# 	shininess .79
						# 	diffuseColor .33 .34 .6
						# 	specularColor .29 .29 .29
						# }
                        texture ImageTexture{
                            url "image/floor.png"
                            repeatS TRUE
                            repeatT TRUE
                        }
                        textureTransform TextureTransform {
                            scale 15 11
                        }
					}
                }
            ]
        }
        #ceil
        Transform {
            translation 7.5 5 5.5
            children [
                Shape {
                    geometry Box {
                        size 15 0.1 11
                    }
					appearance Appearance {
                        texture ImageTexture {
                            url "image/ceil.png"
                        }
                        textureTransform TextureTransform {
                            scale 12 16
                        }
					}
                }
                DEF airfun Transform {
                    translation 0 -0.1 0
                    rotation 0 0 1 3.14159
                    children [
                        Inline {url "airfun.wrl"}
                    ]
                }
                Transform {
                    translation 5 0 0
                    children[
                        USE airfun
                    ]
                }
                Transform {
                    translation -5 0 0
                    children[
                        USE airfun
                    ]
                }
            ]
        }       
        #front
        DEF wallFront Transform {
            translation 0 2.5 5.5
            children [
                Shape {
                    geometry Box {
                        size 0.1 5 11
                    }
					appearance Appearance {
						texture ImageTexture {
                            url "image/wall.png"
                        }
					}
                }
            ]
        }
        #behind
        Transform {
            translation 15 0 0 
            children [
                USE wallFront
            ]
        }
    ]
}

#window-wall
Transform {
    children[
        DEF window Transform {
            translation 1.4 5 0 
            scale 0.16 0.16 0.16
            children [
                Inline {url "window.wrl"}
            ]
        }
        Transform {
            translation 6.1 0 0 
            children [
                USE window
            ]
        }
        Transform {
            translation 12.2 0 0 
            children [
                USE window
            ]
        }
        #wall
        DEF windowWall Transform {
            translation 4.45 2.5 0 
            children [
                Shape {
                    geometry Box {
                        size 3.3 5 0.1
                    }
                    appearance Appearance {
                        texture ImageTexture {
                            url "image/wall.png"
                        }
                    }
                }
            ]
        }
        Transform {
            translation 6.1 0 0
            children [
                USE windowWall
            ]
        }
    ]
}

# door-wall
Transform {
    children [
        #door-left
        DEF doorLeft Transform {
            translation 1.25 5 11
            scale 1.6667 1.6667 1.6667
            children [
                Inline {url "doormirror.wrl"}
            ]
        }
        #door-right 
        Transform {
            translation 12.5 0 0
            children [
                DEF doorRignt Transform {
                    translation 1.25 5 11
                    scale 1.6667 1.6667 1.6667
                    children [
                        Inline {url "door.wrl"}
                    ]
                }
            ]
        }
        #doorWallSide-left
        DEF doorWallSide Transform {
            translation 0.25 2.5 11 
            children [
                Shape {
                    geometry Box {
                        size 0.5 5 0.1
                    }
                    appearance Appearance {
                        texture ImageTexture {
                            url "image/wall.png"
                        }
                    }
                }
            ]
        }
        #doorWallSide-right
        Transform {
            translation 14.5 0 0
            children [
                USE doorWallSide
            ]
        }
        #doorWallMain
        Transform {
            translation 2 0 0 
            scale 22 1 1
            children [
                USE doorWallSide
            ]
        }
    ]
}



#deskAndChair 
Transform {
    children [
        DEF deskAndChair Transform {
            translation  1.5 1.3 1.2 
            scale 0.1667 0.1667 0.1667
            rotation 0 1 0 4.7124
            children [
                Transform {
                    translation 0 0 5
                    rotation 0 1 0 3.1415
                    children [
                        Inline {url "chair.wrl"}
                        
                    ]
                }
                Transform {
                    children [
                        Inline {url "desk.wrl"}
                    ]
                }
            ]
        }

        DEF DACrow Transform {
            children [
                Transform {
                    translation 2 0 0
                    children [
                        USE deskAndChair
                    ]
                }
                Transform {
                    translation 4 0 0
                    children [
                        USE deskAndChair
                    ]
                }
                Transform {
                    translation 6 0 0
                    children [
                        USE deskAndChair
                    ]
                }
                Transform {
                    translation 8 0 0
                    children [
                        USE deskAndChair
                    ]
                }
                Transform {
                    translation 10 0 0
                    children [
                        USE deskAndChair
                    ]
                }
            ]
        }

        Transform {
            translation 0 0 1.8
            children [
                USE deskAndChair
                USE DACrow
            ]
        }

        Transform {
            translation 0 0 3.6
            children [
                USE deskAndChair
                USE DACrow
            ]
        }

        Transform {
            translation 0 0 5.4
            children [
                USE deskAndChair
                USE DACrow
            ]
        }

        Transform {
            translation 0 0 7.2
            children [
                USE deskAndChair
                USE DACrow
            ]
        }

        Transform {
            translation 0 0 9
            children [
                USE DACrow
            ]
        }
    ]
}

